install-openssl = server:.bash_history : CONTAINS : install openssl
ecdh-server-key = server:.bash_history : CONTAINS : openssl ecparam -name secp256k1
ecdh-client-key = client:.bash_history : CONTAINS : openssl ecparam -name secp256k1
shared-secret = server:.bash_history : CONTAINS : openssl pkeyutl -derive -inkey
